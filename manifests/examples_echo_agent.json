{
  "id": "examples_echo_agent",
  "name": "echo_agent",
  "version": "1.0.0",
  "description": "A simple echo agent that responds to messages",
  "entry_point": "examples.echo_agent:echo_agent",
  "module": "examples.echo_agent",
  "function": "echo_agent",
  "capabilities": {
    "cpu_threads": 1,
    "memory_mb": 128,
    "frameworks": ["numpy", "pandas"]
  },
  "config": {
    "max_message_size_kb": 1024,
    "log_level": "INFO"
  },
  "subscribe_patterns": [
    "echo.*"
  ],
  "subscriptions": ["echo.*"],
  "dependencies": [
    "numpy>=1.24.0",
    "pandas>=1.5.0"
  ]
}
